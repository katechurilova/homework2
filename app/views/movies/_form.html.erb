<%= form_for @movie do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :rating %>
  <%= f.text_field :rating %>

  <%= f.label :release_date %>
  <%= f.text_field :release_date %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.submit "Submit" %>
<% end %>
