<%= form_for @movie, :html => {:multipart => true} do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :rating %>
  <%= f.text_field :rating %>

  <%= f.label :release_date %>
  <%= f.text_field :release_date %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label "Picture" %>
  <%= f.file_field :picture %>

  <%= f.submit "Submit" %>
<% end %>
